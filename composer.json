{
  "name" : "Nona Starter Theme",
  "config": {
    "vendor-dir": "lib/vendor",
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    },
    {
      "type" : "package",
      "package" : {
        "name" : "gizburdt/cuztom",
        "version": "2.9.16",
        "dist" : {
           "url"       : "https://github.com/gizburdt/cuztom/archive/master.zip",
          "type"      : "zip"
        }
      }
    },
    {
      "type" : "package",
      "package" : {
        "name" : "webdevstudios/cmb2",
        "version": "2.1.2",
        "dist" : {
           "url"       : "https://github.com/WebDevStudios/CMB2/archive/v2.1.2.zip",
          "type"      : "zip"
        }
      }
    },
    {
      "type" : "package",
      "package" : {
        "name" : "justintadlock/breadcrumb-trail",
        "version": "1.0.0",
        "dist" : {
          "url"         : "https://github.com/justintadlock/breadcrumb-trail/archive/master.zip",
          "type"        : "zip"
        }
      }
    }
  ],
  "require" : {
    "mobiledetect/mobiledetectlib"    : "*",
    "webdevstudios/cmb2"              : "*",
    "gizburdt/cuztom"                 : "*",
    "justintadlock/breadcrumb-trail"  : "*"
  }
}
